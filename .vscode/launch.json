{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run", "dev:ws-prod"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "Graph Demo",
      "program": "${workspaceFolder}/src/graph.ts",
      "type": "node",
      "request": "launch",
      "sourceMaps": true,
      "runtimeArgs": [
        "--inspect-brk",
        "--trace-warnings",
        "--unhandled-rejections=strict",
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "console",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json",
        "NODE_OPTIONS": "-r tsconfig-paths/register",
        "AWS_PROFILE": "admin",
        "AWS_REGION": "us-east-1",
        "NODE_ENV": "development",
        "POSTGRES_USER": "mfiv_user",
        "POSTGRES_PASSWORD": "volatility-db-password",
        "POSTGRES_DB": "volatility_development",
        "POSTGRES_HOST": "postgresd",
        "POSTGRES_PORT": "5432"
      }
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceRoot}/node_modules/moleculer/bin/moleculer-runner.js",
      "sourceMaps": true,
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "cwd": "${workspaceRoot}",
      "args": ["services/**/*.service.ts"]
    },
    {
      "name": "Launch TypeScript",
      "type": "node",
      "request": "launch",
      // "program": "${workspaceRoot}/node_modules/moleculer/bin/moleculer-runner.js",
      "sourceMaps": true,
      "runtimeArgs": [
        "--inspect-brk",
        "--trace-warnings",
        "--unhandled-rejections=strict",
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "${workspaceRoot}/node_modules/moleculer/bin/moleculer-runner.js",
        "--config",
        "moleculer.config.ts",
        "services/**/*.service.ts"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "console",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json",
        "NODE_OPTIONS": "-r tsconfig-paths/register",
        "AWS_PROFILE": "admin",
        "AWS_REGION": "us-east-1",
        "NODE_ENV": "production"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest",
      "program": "${workspaceRoot}/node_modules/jest-cli/bin/jest.js",
      "args": ["--runInBand", "--detectOpenHandles"],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["--inspect-brk", "--nolazy"],
      "outputCapture": "console",
      "env": {
        "AWS_PROFILE": "admin",
        "AWS_REGION": "us-east-1"
      }
    }
  ]
}
