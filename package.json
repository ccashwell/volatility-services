{
  "name": "volatility-services",
  "version": "1.0.11",
  "description": "services to emit mfiv",
  "repository": "github:VolatilityGroup/volatility-services.git",
  "bugs": "https://github.com/VolatilityGroup/volatility-services/issues",
  "config": {
    "port": "3000",
    "aws": {
      "region": "us-east-2",
      "profile": "automation-devops",
      "role": "AdministratorAccess"
    },
    "require": [
      "-r ts-node/register",
      "-r tsconfig-paths/register"
    ],
    "nodejs:debug_args": [
      "--inspect"
    ]
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:production": "tsc -p tsconfig.production.json",
    "dev:working": "NODE_ENV=development AWS_PROFILE=admin AWS_REGION=us-east-1 node -r newrelic -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --hot !services/ws.service.ts services/**/*.service.ts",
    "dev:broken": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --repl --hot services",
    "dev": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot",
    "dev:cron": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/cron.service.ts",
    "dev:rate": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/rate.service.ts",
    "dev:ingest": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/ingest.service.ts",
    "dev:ipfs": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/ipfs.service.ts",
    "dev:accounts": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/accounts.service.js",
    "dev:tokens": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/tokens.service.ts",
    "dev:instrument_info": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --env --repl --hot ./services/instrument_info.service.ts",
    "dev:ws": "node --inspect -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --hot ./services/ws.service.ts",
    "dev:ws-test": "node --inspect -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --env --repl --hot ./services/instrument_info.service.ts ./services/ws.service.ts ./services/index.service.ts ./services/ingest.service.ts ./services/rate.service.ts ./services/cron.service.ts ./services/tokens.service.ts",
    "dev:ws-prod": "NODE_ENV=production SERVICEDIR=dist/services TS_NODE_PROJECT=tsconfig.production.json node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config dist/moleculer.config.js dist/services",
    "docker:dev": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --config moleculer.config.ts",
    "dockerfile:lint": "docker run --platform linux/amd64 --rm -i hadolint/hadolint < Dockerfile",
    "predocker:build": "npm run dockerfile:lint",
    "docker:build": "env-cmd -f .env.dev scripts/docker-build.sh",
    "predocker:push": "npm run docker:build",
    "docker:push": "env-cmd -f .env.dev scripts/docker-push.sh",
    "start": "SERVICEDIR=dist/services TS_NODE_PROJECT=tsconfig.production.json node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config dist/moleculer.config.js dist/services",
    "start:current": "SERVICEDIR=dist/services TS_NODE_BASEURL=./dist node -r tsconfig-paths/register node_modules/.bin/moleculer-runner --config dist/moleculer.config.js",
    "start:old": "SERVICEDIR=dist/services moleculer-runner --config dist/moleculer.config.js",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prepare": "",
    "prod": "SERVICEDIR=dist/services node -r tsconfig-paths/register moleculer-runner dist/moleculer.config.js",
    "cli": "node -r ts-node/register -r tsconfig-paths/register node_modules/.bin/moleculer-runner --repl --config moleculer.config.ts",
    "ci": "jest --watch",
    "test": "NODE_ENV=test jest --colors --verbose=true",
    "test:focus": "NODE_ENV=test jest --verbose=true --runInBand --detectOpenHandles -i test/unit/services/ingest.spec.ts -t 'calls the ipfs job'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles -i test/unit/services/cron.spec.ts -t 'calls the ipfs job'",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "author": "nick@volatility.com",
  "devDependencies": {
    "@types/blessed": "^0.1.19",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.22",
    "@types/sequelize": "^4.28.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-diff": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "sqlite3": "^5.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tscpaths": "^0.0.9"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@fleekhq/fleek-storage-js": "^1.0.21",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.4.1",
    "@types/mkdirp": "^1.0.2",
    "@types/newrelic": "^7.0.3",
    "@types/ws": "^8.5.3",
    "aws-sdk": "^2.1098.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.21.1",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "hashids": "^2.2.10",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.4.4",
    "moleculer": "^0.14.19",
    "moleculer-cron": "npm:@volatility-group/moleculer-cron@^0.0.5",
    "moleculer-db": "^0.8.17",
    "moleculer-db-adapter-typeorm": "^0.1.4",
    "moleculer-repl": "^0.6.6",
    "moleculer-service-ts": "^1.0.14",
    "moleculer-web": "^0.10.4",
    "moleculer-web-uws": "^0.1.1",
    "moment": "^2.29.1",
    "nats": "^2.6.1",
    "neverthrow": "^4.3.1",
    "newrelic": "^8.9.1",
    "node-volatility-mfiv": "npm:@volatility-group/node-volatility-mfiv-internal@^0.2.3-alpha.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "tardis-dev": "^13.1.11",
    "transducist": "^2.2.0",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.3.1",
    "typeorm": "^0.3.2",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "web3": "^1.7.1",
    "ws": "^8.5.0"
  },
  "engines": {
    "node": ">= 16.14"
  },
  "overrides": {
    "uuid": "$uuid",
    "core-js": "$core-js"
  }
}
