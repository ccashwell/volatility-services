{
  "name": "volatility-services",
  "version": "1.0.0",
  "description": "services to emit mfiv",
  "scripts": {
    "build": "tsc",
    "build:production": "tsc -p tsconfig.production.json",
    "dev:working": "NODE_ENV=development AWS_PROFILE=admin AWS_REGION=us-east-1 node -r newrelic -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --hot !services/ws.service.ts services/**/*.service.ts",
    "dev:broken": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --repl --hot services",
    "dev": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --repl --hot",
    "dev:cron": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/cron.service.ts",
    "dev:rate": "AWS_PROFILE=main-admin node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --env --config moleculer.config.ts --repl --hot services/rate.service.ts",
    "dev:instrument_info": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --repl --hot ./services/instrument_info.service.ts",
    "docker:dev": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --hot --config moleculer.config.ts",
    "dockerfile:lint": "docker run --platform linux/amd64 --rm -i hadolint/hadolint < Dockerfile",
    "predocker:build": "npm run dockerfile:lint",
    "docker:build": "env-cmd -f .env.dev scripts/docker-build.sh",
    "predocker:push": "npm run docker:build",
    "docker:push": "env-cmd -f .env.dev scripts/docker-push.sh",
    "start": "TS_NODE_PROJECT=tsconfig.production.json node -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js dist/services",
    "start:current": "SERVICEDIR=dist/services TS_NODE_BASEURL=./dist node -r tsconfig-paths/register node_modules/.bin/moleculer-runner --config dist/moleculer.config.js",
    "start:old": "SERVICEDIR=dist/services moleculer-runner --config dist/moleculer.config.js",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prepare": "",
    "prod": "SERVICEDIR=dist/services node -r tsconfig-paths/register moleculer-runner dist/moleculer.config.js",
    "cli": "node -r ts-node/register -r tsconfig-paths/register node_modules/.bin/moleculer-runner --repl --config moleculer.config.ts",
    "ci": "jest --watch",
    "test": "NODE_ENV=test jest --colors --verbose=true",
    "test:focus": "NODE_ENV=test jest --verbose=true --runInBand --detectOpenHandles -i test/unit/services/ingest.spec.ts -t 'calls the ipfs job'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles -i test/unit/services/cron.spec.ts -t 'calls the ipfs job'",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose -f docker-compose.yml up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/blessed": "^0.1.19",
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.14",
    "@types/sequelize": "^4.28.11",
    "@types/underscore": "^1.11.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.10.1",
    "ejs": "^3.1.6",
    "env-cmd": "^10.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-diff": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "sqlite3": "^5.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "tscpaths": "^0.0.9"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@fleekhq/fleek-storage-js": "^1.0.20",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/newrelic": "^7.0.3",
    "@types/ws": "^8.2.2",
    "aws-sdk": "^2.1067.0",
    "axios": "^0.26.0",
    "dayjs": "^1.10.8",
    "decimal.js": "^10.3.1",
    "dotenv": "^16.0.0",
    "ioredis": "^4.28.5",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "moleculer": "^0.14.19",
    "moleculer-cron": "file:../moleculer-cron",
    "moleculer-db": "^0.8.17",
    "moleculer-db-adapter-typeorm": "^0.1.4",
    "moleculer-repl": "^0.6.6",
    "moleculer-service-decorators": "^1.0.4",
    "moleculer-service-ts": "^1.0.14",
    "moleculer-web": "^0.10.4",
    "moleculer-web-uws": "^0.1.1",
    "nats": "^2.4.0",
    "neverthrow": "^4.3.1",
    "newrelic": "^8.7.1",
    "node-volatility-mfiv": "file:node-volatility-mfiv-internal",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "tardis-dev": "^13.1.10",
    "transducist": "^2.2.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.6.2",
    "web3": "^1.7.0",
    "ws": "^8.4.2"
  },
  "engines": {
    "node": ">= 16.14"
  }
}
