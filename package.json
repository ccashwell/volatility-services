{
  "name": "volatility-mfiv-index",
  "version": "1.0.0",
  "description": "mfiv methodology",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev:half": "POSTGRES_HOST=0.0.0.0 NODE_ENV=development AWS_PROFILE=admin AWS_REGION=us-east-1 node --inspect -r newrelic -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --repl --config moleculer.config.ts --hot",
    "dev:working": "NODE_ENV=development AWS_PROFILE=admin AWS_REGION=us-east-1 node -r newrelic -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --hot !services/ws.service.ts services/**/*.service.ts",
    "dev:broken": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --repl --hot services",
    "dev": "node -r newrelic -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --repl --hot",
    "start": "SERVICEDIR=dist/services TS_NODE_BASEURL=./dist node -r ts-node/register -r tsconfig-paths/register ./node_modules/moleculer/bin/moleculer-runner.js --config dist/moleculer.config.js",
    "start:current": "SERVICEDIR=dist/services TS_NODE_BASEURL=./dist node -r tsconfig-paths/register node_modules/.bin/moleculer-runner --config dist/moleculer.config.js",
    "start:old": "SERVICEDIR=dist/services moleculer-runner --config dist/moleculer.config.js",
    "prod": "SERVICEDIR=dist/services node -r tsconfig-paths/register moleculer-runner dist/moleculer.config.js",
    "start:sub": "AWS_PROFILE=vg AWS_REGION=us-east-1 ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/ingest.service.ts services/instrument.service.ts services/ipfs.service.ts services/index.service.ts",
    "deps": "npm-check -u",
    "cli": "node -r ts-node/register -r tsconfig-paths/register node_modules/.bin/moleculer-runner --repl --config moleculer.config.ts",
    "ci": "jest --watch",
    "test": "NODE_ENV=test jest --colors --verbose=true",
    "test:focus": "NODE_ENV=test jest --verbose=true --runInBand --detectOpenHandles -i test/unit/services/ingest.spec.ts -t 'calls the ipfs job'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles -i test/unit/services/cron.spec.ts -t 'calls the ipfs job'",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose -f docker-compose.env.yml up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.14",
    "@types/sequelize": "^4.28.11",
    "@types/underscore": "^1.11.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.10.1",
    "ejs": "^3.1.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  },
  "dependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.20",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/newrelic": "^7.0.3",
    "@types/ws": "^8.2.2",
    "aws-sdk": "^2.1067.0",
    "axios": "^0.25.0",
    "chalk": "^5.0.0",
    "decimal.js": "^10.3.1",
    "ioredis": "^4.28.5",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "moleculer": "^0.14.19",
    "moleculer-cron": "file:../moleculer-cron",
    "moleculer-db": "^0.8.17",
    "moleculer-db-adapter-typeorm": "^0.1.4",
    "moleculer-repl": "^0.6.6",
    "moleculer-service-decorators": "^1.0.4",
    "moleculer-service-ts": "^1.0.14",
    "moleculer-web": "^0.10.4",
    "moleculer-web-uws": "^0.1.1",
    "nats": "^1.4.12",
    "neverthrow": "^4.3.1",
    "newrelic": "^8.7.1",
    "node-volatility-mfiv": "github:volatilitygroup/node-volatility-mfiv#release/initial",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "tardis-dev": "13.1.7",
    "transducist": "^2.2.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5",
    "web3": "^1.7.0",
    "ws": "^8.4.2"
  },
  "engines": {
    "node": ">= 16.13.x",
    "npm": "8.4.1"
  }
}
