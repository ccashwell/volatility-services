{
  "name": "volatility-mfiv-index",
  "version": "1.0.0",
  "description": "mfiv methodology",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev": "AWS_PROFILE=vg AWS_REGION=us-east-1 ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "start": "SERVICEDIR=dist/services moleculer-runner --config dist/moleculer.config.js",
    "start:ingest": "AWS_PROFILE=vg AWS_REGION=us-east-1 ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/ingest.service.ts services/instrument.service.ts",
    "deps": "npm-check -u",
    "cli": "AWS_PROFILE=vg AWS_REGION=us-east-1 moleculer connect ",
    "ci": "jest --watch",
    "test": "AWS_PROFILE=vg AWS_REGION=us-east-1 jest --coverage",
    "test:focus": "AWS_PROFILE=vg AWS_REGION=us-east-1 NODE_ENV=test jest --verbose=true --runInBand --detectOpenHandles -i test/unit/services/ingest.spec.ts -t 'calls the ipfs job'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles -i test/unit/services/cron.spec.ts -t 'calls the ipfs job'",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down",
    "sequelize": "sequelize"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.9",
    "@types/sequelize": "^4.28.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "ejs": "^3.1.6",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "moleculer-repl": "^0.6.2",
    "prettier": "^2.5.1",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.20",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^25.1.4",
    "@types/mkdirp": "^1.0.0",
    "@types/ws": "^8.2.2",
    "@volatilitygroup/moleculer-cron": "github:volatilitygroup/moleculer-cron",
    "aws-sdk": "^2.1058.0",
    "axios": "^0.24.0",
    "moleculer": "^0.14.0",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-sequelize": "^0.2.13",
    "moleculer-web": "^0.9.0",
    "moleculer-web-uws": "^0.1.1",
    "neverthrow": "^4.3.1",
    "node-volatility-mfiv": "github:volatilitygroup/node-volatility-mfiv#3e7964759",
    "npm-check": "^5.9.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.13.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.2",
    "tardis-dev": "^13.1.6",
    "transducist": "^2.2.0",
    "typescript": "^3.8.3",
    "ws": "^7.5.6"
  },
  "engines": {
    "node": "^16.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
