{
  "name": "volatility-mfiv-index",
  "version": "1.0.0",
  "description": "mfiv methodology",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev": "ts-node -r tsconfig-paths/register  --project tsconfig.json ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "start": "SERVICEDIR=dist/services moleculer-runner --config dist/moleculer.config.js",
    "start:sub": "AWS_PROFILE=vg AWS_REGION=us-east-1 ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/ingest.service.ts services/instrument.service.ts services/ipfs.service.ts services/index.service.ts",
    "deps": "npm-check -u",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "test:focus": "NODE_ENV=test jest --verbose=true --runInBand --detectOpenHandles -i test/unit/services/ingest.spec.ts -t 'calls the ipfs job'",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles -i test/unit/services/cron.spec.ts -t 'calls the ipfs job'",
    "lint": "eslint --ext .js,.ts .",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.13",
    "@types/sequelize": "^4.28.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "ejs": "^3.1.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "moleculer-repl": "^0.7.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.12.0"
  },
  "dependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.20",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.4.0",
    "@types/mkdirp": "^1.0.2",
    "@types/ws": "^8.2.2",
    "aws-sdk": "^2.1065.0",
    "blerg": "^0.0.3",
    "decimal.js": "^10.3.1",
    "log4js": "^6.4.1",
    "moleculer": "^0.14.19",
    "moleculer-cron": "file:../moleculer-cron",
    "moleculer-db": "^0.8.17",
    "moleculer-db-adapter-typeorm": "^0.1.4",
    "moleculer-web": "^0.10.4",
    "moleculer-web-uws": "^0.1.1",
    "neverthrow": "^4.3.1",
    "newrelic": "^8.7.1",
    "node-volatility-mfiv": "github:volatilitygroup/node-volatility-mfiv#release/initial",
    "npm-check": "^5.9.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "tardis-dev": "13.1.7",
    "transducist": "^2.2.0",
    "typeorm": "^0.2.41",
    "typescript": "^3.8.3",
    "ws": "^8.4.2"
  },
  "engines": {
    "node": ">= 16.13.x",
    "npm": "8.1.2"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^@(jest)/(.*)$": "<rootDir>/node_modules/@jest/$2",
      "^@(babel)/(.*)$": "<rootDir>/node_modules/@babel/$2",
      "^@(test)(.*)$": "<rootDir>/test/$2",
      "@entities/connection": "<rootDir>/test/config/connection",
      "^@([a-z].*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    }
  }
}
